  <actionParam paramType="url" paramValue="https://www.tb1.rcdnlab1.com" />
  <actionParam paramType="username" paramValue="var1" />
  <actionParam paramType="password" paramValue="Test1234" />
  <actionParam paramType="type" paramValue="Agent" />
 </action>
 <action acID="2" stoptype="abort" timeout="">
 	<actionName>goTo</actionName>
 	<actionParam paramType="type" paramValue="page" />
