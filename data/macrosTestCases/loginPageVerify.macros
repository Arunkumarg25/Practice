workflow(driver=selenium, wfid=loginPageVerify, stoptype=abort, profile=webdriver)
config(path=./data/selenium_config/page_log_in.xml)
navigate(url=https://www.t5a.l1.cisco.com)
input(type=text field, label=Username, value=jearnold_1)
